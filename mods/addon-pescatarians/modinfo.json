{
  "src": [ "../../shared", "." ],
  "converter": [
    {
      "action": "static",
      "pattern": "products/**/{*.include.xml,icon_*.png}"
    },
    {
      "action": "static",
      "pattern": "data/**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.*,README.md,data/**/icon_*.png,features.include.xml}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "gltf",
      "pattern": "**/*.gltf",
      "lods": 4,
      "changePath": "rdm/",
      "animPath": "anim/"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo"
    }
  ],
  "Version": "2.1",
  "ModID": "jakob_pescatarians",
  "ModDependencies": [ "jakob_extra_goods" ],
  "Category": {
    "English": "Addon"
  },
  "ModName": {
    "English": "Pescatarians (Jakob)",
    "German": "Pescetarier (Jakob)"
  },
  "DLCDependencies": [
    {
      "DLC": "LandOfLions",
      "Dependant": "atLeastOneRequired"
    },
    {
      "DLC": "Docklands",
      "Dependant": "atLeastOneRequired"
    }
  ],
  "CreatorName": "Jakob",
  "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods"
}
