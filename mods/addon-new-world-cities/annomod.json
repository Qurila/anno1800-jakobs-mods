{
  "src": [ ".", "../../shared" ],
  "out": "${annoMods}/${modName}",
  "converter": [
    {
      "action": "static",
      "pattern": "pools/**/{*.include.xml,icon_*.png}"
    },
    {
      "action": "static",
      "pattern": "data/**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.*,README.md,data/**/icon_*.png,features.include.xml}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "gltf",
      "pattern": "**/!(propsonly)*.gltf",
      "lods": 4,
      "changePath": "rdm/",
      "animPath": "anim/"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask,rga}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1.2",
    "ModID": "jakob_new_world_cities",
    "DeprecateIds": [ "jakob_nw_construction", "jakob_bugfix_celluloid", "jakob_power_nw" ],
    "IncompatibleIds": [],
    "ModDependencies": [ ],
    "Category": {
      "English": "Addon"
    },
    "ModName": {
      "English": "New World Cities (Jakob)"
    },
    "Description": {
      "English": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "SeedsOfChange",
        "Dependant": "partly"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}