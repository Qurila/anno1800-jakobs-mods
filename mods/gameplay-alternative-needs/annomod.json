{
  "src": [ "../../shared", "." ],
  "out": "${annoMods}/${modName}",
  "converter": [
    {
      "action": "static",
      "pattern": "products/**/{*.include.xml,icon_*.png}"
    },
    {
      "action": "static",
      "pattern": "data/**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.*,README.md,**/icon_*.png,features.include.xml}"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "modinfo"
    }
  ],
  "modinfo": {
    "Version": "1.2.2",
    "ModID": "jakob_alternative_needs",
    "ModDependencies": [ "jakob_extra_goods" ],
    "LoadAfterIds": [ "*" ],
    "Category": {
      "English": "Gameplay"
    },
    "ModName": {
      "English": "Alternative Needs (Jakob)"
    },
    "Description": {
      "English": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "Docklands",
        "Dependant": "partly"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}