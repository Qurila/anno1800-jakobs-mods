{
  "src": [ "../../shared", "." ],
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "products/**/{*.include.xml,icon_*.png}"
    },
    {
      "action": "static",
      "pattern": "data/**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.*,README.md,data/**/icon_*.png,**/icons/*.png,features.include.xml}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask,rga,r_a,r_a+b}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "gltf",
      "pattern": "**/!(propsonly)*.gltf",
      "lods": 5,
      "changePath": "rdm/",
      "animPath": "anim/"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "2.3.3",
    "ModID": "jakob_upgradable_low_tier",
    "IncompatibleIds": [],
    "ModDependencies": [ "jakob_extra_goods", "jakob_ground_textures" ],
    "Category": {
      "English": "Addon"
    },
    "ModName": {
      "English": "Upgradable Workers (Jakob)"
    },
    "Description": {
      "English": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "Highlife",
        "Dependant": "required"
      },
      {
        "DLC": "Docklands",
        "Dependant": "partly"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}