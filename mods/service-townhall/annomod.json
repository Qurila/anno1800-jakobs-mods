{
  "src": ".",
  "out": "${annoMods}/[Skin] Everything Skin Pack (Jakob)/${modName}",
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.*,README.md,**/icon_*.png}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "gltf",
      "pattern": "**/*.{glb,gltf}",
      "lods": 5,
      "changePath": "rdm/",
      "animPath": "anim/"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "2.0",
    "ModID": "jakob_New_Town_Hall",
    "IncompatibleIds": [],
    "ModDependencies": null,
    "Category": {
      "English": "Skin"
    },
    "ModName": {
      "English": "New Town Halls (Jakob)",
      "German": "Neues Rath√§user (Jakob)"
    },
    "Description": {
      "English": "README.md",
      "German": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [
      {
        "DLC": "Highlife",
        "Dependant": "atLeastOneRequired"
      },
      {
        "DLC": "LandOfLions",
        "Dependant": "atLeastOneRequired"
      }
    ],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}