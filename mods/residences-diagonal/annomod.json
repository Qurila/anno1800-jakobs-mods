{
  "src": ".",
  "out": "${annoMods}/${modName}",  
  "converter": [
    {
      "action": "static",
      "pattern": "**/*.{cfg,ifo,prp,fc,rdm,dds,include.xml}"
    },
    {
      "action": "static",
      "pattern": "{data/config/**/*,banner.png,README.md,**/icon*.png}"
    },
    {
      "action": "cf7",
      "pattern": "**/*.cf7"
    },
    {
      "action": "texture",
      "pattern": "**/*_{diff,norm,height,metal,mask}.png",
      "lods": 3,
      "changePath": "maps/"
    },
    {
      "action": "gltf",
      "pattern": "data/jakob/**/*.gltf",
      "lods": 5,
      "changePath": "rdm/",
      "animPath": "anim/"
    },
    {
      "action": "cfgyaml",
      "pattern": "**/*.cfg.yaml"
    },
    {
      "action": "modinfo",
      "content_en": true
    }
  ],
  "modinfo": {
    "Version": "1.5",
    "ModID": "jakob_diagonal_residences",
    "IncompatibleIds": [],
    "ModDependencies": [],
    "Category": {
      "English": "Building"
    },
    "ModName": {
      "English": "Diagonal Residences (Jakob)"
    },
    "Description": {
      "English": "README.md",
      "German": "README.md"
    },
    "KnownIssues": null,
    "DLCDependencies": [],
    "CreatorName": "Jakob",
    "CreatorContact": "https://github.com/jakobharder/anno-1800-jakobs-mods",
    "Image" : null
  }
}